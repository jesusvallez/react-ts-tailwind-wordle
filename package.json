{
  "name": "wordle",
  "version": "2.0.0",
  "author": {
    "name": "Jesús Vállez",
    "email": "jesusvg91@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jesusvallez/react-ts-tailwind-wordle.git"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc --project tsconfig.build.json && vite build",
    "preview": "vite preview",
    "release:major": "npx changelog -M && npm run release:commit && npm version major",
    "release:minor": "npx changelog -m && npm run release:commit && npm version minor",
    "release:patch": "npx changelog -p && npm run release:commit && npm version patch",
    "release:commit": "git add CHANGELOG.md && git commit -m 'docs(changelog): updated CHANGELOG.md'",
    "push:tag": "git push origin && git push origin --tags",
    "test": "tsc && jest",
    "lint": "eslint src/. --fix"
  },
  "dependencies": {
    "clsx": "1.2.1",
    "react": "18.2.0",
    "react-canvas-confetti": "1.3.0",
    "react-dom": "18.2.0",
    "vite-plugin-svgr": "2.4.0"
  },
  "devDependencies": {
    "@commitlint/cli": "17.4.4",
    "@commitlint/config-conventional": "17.4.4",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "14.0.0",
    "@types/jest": "29.4.0",
    "@types/node": "18.14.6",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "@types/testing-library__jest-dom": "5.14.5",
    "@types/testing-library__react": "10.2.0",
    "@typescript-eslint/eslint-plugin": "5.54.0",
    "@typescript-eslint/parser": "5.54.0",
    "@vitejs/plugin-react": "3.1.0",
    "autoprefixer": "10.4.13",
    "eslint": "8.35.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-config-standard": "17.0.0",
    "eslint-config-standard-typescript-prettier": "6.0.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-unused-imports": "2.0.0",
    "husky": "8.0.3",
    "jest": "29.4.3",
    "jest-environment-jsdom": "29.4.3",
    "postcss": "8.4.21",
    "prettier": "2.8.4",
    "prettier-plugin-tailwindcss": "0.2.4",
    "tailwindcss": "3.2.7",
    "ts-jest": "29.0.5",
    "typescript": "4.9.5",
    "vite": "4.1.4",
    "vite-tsconfig-paths": "4.0.5"
  }
}
